{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/web/src",
  "prefix": "shinysorter",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "inputs": [
        "{projectRoot}/src/app/**",
        "{projectRoot}/src/assets/app.config.json",
        "{projectRoot}/src/styles.sass"
      ],
      "options": {
        "outputPath": "dist/apps/web",
        "index": "apps/web/src/index.html",
        "main": "apps/web/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/web/tsconfig.app.json",
        "inlineStyleLanguage": "sass",
        "assets": [
          "apps/web/src/favicon.ico",
          "apps/web/src/assets"
        ],
        "styles": [
          "apps/web/src/styles.sass",
          "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
        ],
        "baseHref": "/replacemebasehref/",
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "docker-build": {
      "inputs": [
        "default"
      ],
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-commands",
      "options": {
        "envFile": "apps/web/.env.default",
        "commands": [
          "docker build -f apps/web/Dockerfile . -t $WEB_DOCKER_IMG:$WEB_DOCKER_TAG"
        ]
      },
      "configurations": {
        "production": {
          "envFile": "apps/web/.env.production"
        }
      }
    },
    "docker-push": {
      "dependsOn": [
        "docker-build"
      ],
      "executor": "nx:run-commands",
      "options": {
        "envFile": "apps/web/.env.default",
        "commands": [
          "docker push $WEB_DOCKER_IMG:$WEB_DOCKER_TAG"
        ]
      },
      "configurations": {
        "production": {
          "envFile": "apps/web/.env.production"
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "development": {
          "browserTarget": "web:build:development",
          "open": true
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/web/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "web:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/web/**/*.ts",
          "apps/web/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/web/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}